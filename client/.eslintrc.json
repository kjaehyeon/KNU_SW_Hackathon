{
    "extends": [
        "react-app",
        "google"
    ],
    "rules": {
        "indent": [
            "error",
            2
        ],
        "camelcase": "off",
        "new-cap": "off",
        "require-jsdoc": "off",
        "max-len": [
            "error",
            90
        ],
        "no-unused-vars": "off",
        "react-hooks/exhaustive-deps": "off",
        "linebreak-style": "off"
    }
}